<UserControl
	x:Class="Mntone.MonitorInformation.Views.Monitor63"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:local="clr-namespace:Mntone.MonitorInformation.Views">
	
	<Grid>
		<Grid.RowDefinitions>
			<RowDefinition Height="4"/>
			<RowDefinition Height="Auto"/>
			<RowDefinition Height="4"/>
			<RowDefinition Height="Auto"/>
			<RowDefinition Height="4"/>
			<RowDefinition Height="Auto"/>
		</Grid.RowDefinitions>
		<Grid.ColumnDefinitions>
			<ColumnDefinition Width="120"/>
			<ColumnDefinition/>
		</Grid.ColumnDefinitions>

		<TextBlock Grid.Row="1" Text="Effective dpi: "/>
		<TextBox Grid.Row="1" Grid.Column="1" Text="{Binding EffectiveDpi, Mode=OneTime}"/>

		<TextBlock Grid.Row="3" Text="Angular dpi: "/>
		<TextBox Grid.Row="3" Grid.Column="1" Text="{Binding AngularDpi, Mode=OneTime}"/>

		<TextBlock Grid.Row="5" Text="Raw dpi: "/>
		<TextBox Grid.Row="5" Grid.Column="1" Text="{Binding RawDpi, Mode=OneTime}"/>
	</Grid>
</UserControl>
